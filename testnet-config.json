{
    "remoteServerUrl": "https:///api-testnet.ergoplatform.com/",
    "proxyPort": 9001,
    "inputHarFile": "./testnet.har",
    "cacheEverything": true,
    "cacheBustingParams": ["_", "dc", "cacheSlayer"],
    "excludedRouteMatchers": ["/*.js", "/*.png"],
    "excludedStatusMatchers": ["503", "404"],
    "showConsoleOutput": true,
    "dataPlayback": true,
    "dataRecord": true,
    "commandPrefix": "proxy",
    "proxyHeaderIdentifier": "proxy-cache-playback",
    "proxyTimeout": 500000,
    "deleteCookieDomain": true,
    "overrideCors": "localhost:8081",
    "useCorsCredentials": true
}

